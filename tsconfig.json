{
  "compilerOptions": {
    "target": "esnext",
    "module": "esnext",
    "moduleResolution": "node",
    "strict": true,
    "jsx": "preserve",
    "sourceMap": true,
    "resolveJsonModule": true,
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "esModuleInterop": true,
    "noImplicitAny": false,
    "baseUrl": "./",
    "allowJs": true,
    "skipLibCheck": true,
    "jsxImportSource": "vue",
    "noEmit": true,
    "skipDefaultLibCheck": true,
    "lib": [
      "esnext",
      "dom",
      "dom.iterable"
    ],
    "types": [],
    "paths": {
      "~": [
        "./src"
      ],
      "~/*": [
        "./src/*"
      ]
    }
  },
  "include": [
    "src/**/*.ts",
    "src/**/*.d.ts",
    "src/**/*.tsx",
    "src/**/*.vue",
  ],
  "exclude": [
    "node_modules",
    "assets",
    "src/components/function-editor/libs/node/**/*",
    "dist/**"
  ]
}